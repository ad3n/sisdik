<?php

namespace Fast\SisdikBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Fast\SisdikBundle\Entity\Siswahadir
 *
 * @ORM\Table(name="siswahadir")
 * @ORM\Entity
 */
class Siswahadir
{
    /**
     * @var integer $id
     *
     * @ORM\Column(name="id", type="bigint", nullable=false)
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="IDENTITY")
     */
    private $id;

    /**
     * @var \DateTime $tanggal
     *
     * @ORM\Column(name="tanggal", type="date", nullable=true)
     */
    private $tanggal;

    /**
     * @var boolean $hadir
     *
     * @ORM\Column(name="hadir", type="boolean", nullable=true)
     */
    private $hadir;

    /**
     * @var integer $telatDlr
     *
     * @ORM\Column(name="telat_dlr", type="integer", nullable=true)
     */
    private $telatDlr;

    /**
     * @var \DateTime $telatDlrtime
     *
     * @ORM\Column(name="telat_dlrtime", type="datetime", nullable=true)
     */
    private $telatDlrtime;

    /**
     * @var integer $absenDlr
     *
     * @ORM\Column(name="absen_dlr", type="integer", nullable=true)
     */
    private $absenDlr;

    /**
     * @var \DateTime $absenDlrtime
     *
     * @ORM\Column(name="absen_dlrtime", type="datetime", nullable=true)
     */
    private $absenDlrtime;



    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set tanggal
     *
     * @param \DateTime $tanggal
     * @return Siswahadir
     */
    public function setTanggal($tanggal)
    {
        $this->tanggal = $tanggal;
    
        return $this;
    }

    /**
     * Get tanggal
     *
     * @return \DateTime 
     */
    public function getTanggal()
    {
        return $this->tanggal;
    }

    /**
     * Set hadir
     *
     * @param boolean $hadir
     * @return Siswahadir
     */
    public function setHadir($hadir)
    {
        $this->hadir = $hadir;
    
        return $this;
    }

    /**
     * Get hadir
     *
     * @return boolean 
     */
    public function getHadir()
    {
        return $this->hadir;
    }

    /**
     * Set telatDlr
     *
     * @param integer $telatDlr
     * @return Siswahadir
     */
    public function setTelatDlr($telatDlr)
    {
        $this->telatDlr = $telatDlr;
    
        return $this;
    }

    /**
     * Get telatDlr
     *
     * @return integer 
     */
    public function getTelatDlr()
    {
        return $this->telatDlr;
    }

    /**
     * Set telatDlrtime
     *
     * @param \DateTime $telatDlrtime
     * @return Siswahadir
     */
    public function setTelatDlrtime($telatDlrtime)
    {
        $this->telatDlrtime = $telatDlrtime;
    
        return $this;
    }

    /**
     * Get telatDlrtime
     *
     * @return \DateTime 
     */
    public function getTelatDlrtime()
    {
        return $this->telatDlrtime;
    }

    /**
     * Set absenDlr
     *
     * @param integer $absenDlr
     * @return Siswahadir
     */
    public function setAbsenDlr($absenDlr)
    {
        $this->absenDlr = $absenDlr;
    
        return $this;
    }

    /**
     * Get absenDlr
     *
     * @return integer 
     */
    public function getAbsenDlr()
    {
        return $this->absenDlr;
    }

    /**
     * Set absenDlrtime
     *
     * @param \DateTime $absenDlrtime
     * @return Siswahadir
     */
    public function setAbsenDlrtime($absenDlrtime)
    {
        $this->absenDlrtime = $absenDlrtime;
    
        return $this;
    }

    /**
     * Get absenDlrtime
     *
     * @return \DateTime 
     */
    public function getAbsenDlrtime()
    {
        return $this->absenDlrtime;
    }
}