    '@FastSisdikBundle/Resources/public/js/custom.js'
%}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

{% javascripts '@FastSisdikBundle/Resources/public/js/libraries/jquery.pngFix.pack.js' %}
<script type="text/javascript" src="{{ asset_url }}"></script>
{% endjavascripts %}

{% endblock javascripts_top %}

{% block additional_resources %}
{% block javascripts_add %}{% endblock javascripts_add %}
{% block stylesheets_add %}{% endblock stylesheets_add %}
{% endblock additional_resources %}

{% block custom_body_tag %}<body>{% endblock %}

