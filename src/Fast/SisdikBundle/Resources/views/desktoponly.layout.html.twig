{% extends '::base.html.twig' %}

{% block stylesheets %}
    {% stylesheets
        'bundles/fastsisdik/css/screen.css'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <!--[if IE]>
    {% stylesheets 'bundles/fastsisdik/css/pro_dropline_ie.css' filter='cssrewrite' %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
    <![endif]-->
    
    
    {% stylesheets
        'bundles/fastsisdik/js/libraries/themes/smoothness/jquery-ui-1.9.1.custom.css'
        filter='cssrewrite'
    %}
        <link rel="stylesheet" href="{{ asset_url }}" />
    {% endstylesheets %}
{% endblock %}

{% block javascripts_top %}

{% javascripts
    '@FastSisdikBundle/Resources/public/js/libraries/jquery-1.8.2.min.js'
