{% extends '::base.html.twig' %}

{% block body -%}
    <h1>SekolahAsal</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nama</th>
                <td>{{ entity.nama }}</td>
            </tr>
            <tr>
                <th>Kode</th>
                <td>{{ entity.kode }}</td>
            </tr>
            <tr>
                <th>Alamat</th>
                <td>{{ entity.alamat }}</td>
            </tr>
            <tr>
                <th>Penghubung</th>
                <td>{{ entity.penghubung }}</td>
            </tr>
            <tr>
                <th>Ponselpenghubung</th>
                <td>{{ entity.ponselPenghubung }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('sekolahasal') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('sekolahasal_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('sekolahasal_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
