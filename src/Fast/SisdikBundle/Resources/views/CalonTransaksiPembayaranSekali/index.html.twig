<h1>CalonTransaksiPembayaranSekali list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Nominalpembayaran</th>
            <th>Keterangan</th>
            <th>Waktusimpan</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('applicant_oncefee_transaction_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.nominalPembayaran }}</td>
            <td>{{ entity.keterangan }}</td>
            <td>{% if entity.waktuSimpan %}{{ entity.waktuSimpan|date('Y-m-d H:i:s') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('applicant_oncefee_transaction_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('applicant_oncefee_transaction_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('applicant_oncefee_transaction_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
